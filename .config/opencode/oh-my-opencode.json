      "model": "opencode/kimi-k2.5"
    },
    "OpenCode-Builder": {
      "model": "opencode/kimi-k2.5"
    },
    "prometheus": {
      "model": "opencode/kimi-k2.5"
    },
    "metis": {
      "model": "opencode/kimi-k2.5"
    },
    "momus": {
      "model": "opencode/kimi-k2.5"
    },
    "oracle": {
      "model": "opencode/kimi-k2.5"
    },
    "librarian": {
      "model": "opencode/kimi-k2.5"
    },
    "explore": {
      "model": "opencode/kimi-k2.5"
    },
    "multimodal-looker": {
      "model": "opencode/kimi-k2.5"
    },
    "general": {
      "model": "opencode/kimi-k2.5"
    },
    "deep": {
